    <referenceContainer name="udropship.content">
      <block class="Magento\Framework\View\Element\Template" template="Unirgy_Dropship::unirgy/dropship/vendor/product/wrapper.phtml" name="wrapper" cacheable="false">
        <block class="Unirgy\Dropship\Block\Vendor\Product\Pager" name="product.grid.toolbar"/>
